  <DataSets>
    <DataSet Name="members">
      <Query>
        <DataSourceName>league</DataSourceName>
        <CommandText>select  t.team_name as team
       ,b.bowler_name as bowler
	   ,b.address
	   ,b.phone
	   ,b.email
from   hba_bowler b
join   hba_team   t on t.id = b.team_id
where  b.active_flag = 'Y'
and      t.id &lt;&gt; 1
order by t.team_name, b.position</CommandText>
      </Query>
      <Fields>
        <Field Name="team">
          <DataField>team</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="bowler">
          <DataField>bowler</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="address">
          <DataField>address</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="phone">
          <DataField>phone</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="email">
          <DataField>email</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
