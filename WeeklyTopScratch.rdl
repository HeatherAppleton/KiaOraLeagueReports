  <DataSets>
    <DataSet Name="Ladies">
      <Query>
        <DataSourceName>League</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@p_date">
            <Value>=Parameters!p_date.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select  top 4 
           ig.*,
           b.gender,
	   b.bowler_name
from   hba_v_individual_game ig 
join   hba_week   w  on w.season      = ig.season 
                    and w.week_number = ig.week_number
join   hba_bowler b  on b.id = ig.bowler_id
where b.gender = 'F'
and    w.week_date = @p_date
order by ig.game_1 desc, ig.game</CommandText>
      </Query>
      <Fields>
        <Field Name="season">
          <DataField>season</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="week_number">
          <DataField>week_number</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
