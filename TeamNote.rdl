  <DataSets>
    <DataSet Name="TeamNote">
      <Query>
        <DataSourceName>league</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@p_season">
            <Value>=Parameters!p_season.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@p_team_id">
            <Value>=Parameters!p_team_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select tn.note 
from  hba_team_note tn 
where tn.season = @p_season 
and    tn.team_id = @p_team_id
order by tn.rank</CommandText>
      </Query>
