    <DataSet Name="team">
      <Query>
        <DataSourceName>league</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@p_week_id">
            <Value>=Parameters!p_week_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select  d.team_name 
      ,d.round_winner
       ,sum(d.calculated_total) as points 
from   hba_v_draw d 
join   (select  w.season 
			   ,w.round
		from   hba_v_week w
		where  w.id = @p_week_id
	   ) s  on d.season = s.season 
	       and d.round  = s.round
group by d.team_name  ,d.round_winner
order by 3 desc, d.team_name</CommandText>
      </Query>
      <Fields>
        <Field Name="team_name">
          <DataField>team_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="round_winner">
          <DataField>round_winner</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="points">
          <DataField>points</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="ladies">
      <Query>
        <DataSourceName>league</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@p_week_id">
            <Value>=Parameters!p_week_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select  top 4
        b.bowler_name as Bowler
       ,ig.game_1 as Score 
from   hba_v_week w 
join   hba_v_individual_game ig  on w.season      = ig.season 
                                and w.week_number = ig.week_number
join   hba_bowler b on b.id = ig.bowler_id
where  w.id = @p_week_id
and    b.gender = 'F'
order by ig.game_1 desc, b.bowler_name</CommandText>
      </Query>
      <Fields>
        <Field Name="Bowler">
          <DataField>Bowler</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Score">
          <DataField>Score</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="men">
      <Query>
        <DataSourceName>league</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@p_week_id">
            <Value>=Parameters!p_week_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select  top 4
        b.bowler_name as Bowler
       ,ig.game_1 as Score 
from   hba_v_week w 
join   hba_v_individual_game ig  on w.season      = ig.season 
                                and w.week_number = ig.week_number
join   hba_bowler b on b.id = ig.bowler_id
where  w.id = @p_week_id
and    b.gender = 'M'
order by ig.game_1 desc, b.bowler_name</CommandText>
      </Query>
